@model List<ProyectoPasteleria.Models.Catalogo>
@{
    ViewBag.Title = "PRODUCTOS";

}
<h1>ViewBag.Title</h1>

<div class="row">
    <div class="col-mg-12">
        <table>
            <tr>
                <th>#</th>
                <th>nombre</th>
            </tr>
            @foreach (var elemento in Model)
            {
                <tr>
                    <td>@elemento.ID_CATALOGO</td>
                    <td>@elemento.NOMBRE_CATALOGO</td>
                </tr>
            }
        </table>

    </div>




</div>